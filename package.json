{
  "name": "assemblyre",
  "version": "0.0.1",
  "description": "Editor environment for creating pseudo-programs",
  "main": "server.js",
  "scripts": {
    "start": "npm run compile && node server.js",
    "compile": "browserify -t vueify -e src/main.js -o public/build.js"
  },
  "dependencies": {
    "express": "^4.18.1",
    "browserify": "^17.0.0",
    "vueify": "^9.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-core": "^6.26.3",
    "vue": "2.6.8",
    "vue-template-compiler": "2.5.15",
    "interactjs": "1.10.11",
    "marked": "4.0.16",
    "dompurify": "2.3.8"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "vue"
  ]
}
